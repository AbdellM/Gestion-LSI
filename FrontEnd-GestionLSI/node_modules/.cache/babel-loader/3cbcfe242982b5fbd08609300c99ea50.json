 :objects=\"Admins\"\r\n      type=\"admin\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ShowTable from \"@/components/ShowTable.vue\";\r\n\r\nexport default {\r\n  name: \"ShowUser\",\r\n  components: {\r\n    ShowTable,\r\n  },\r\n  data() {\r\n    return {\r\n      role: \"\",\r\n      Students: [],\r\n      Profs: [],\r\n      Admins: [],\r\n      tableHeaderStudent: [\r\n        \"Nom\",\r\n        \"Prenom\",\r\n        \"Email\",\r\n        \"Date de naissance\",\r\n        \"CNE\",\r\n        \"Semestre\",\r\n      ],\r\n      tableHeaderProf: [\"Nom\", \"Prenom\", \"Email\", \"Modules\"],\r\n      tableHeaderAdmin: [\"Nom\", \"Prenom\", \"Email\"],\r\n    };\r\n  },\r\n  methods: {\r\n    getAllUsers() {\r\n      switch (this.role) {\r\n        case \"student\":\r\n          this.$store.dispatch(\"getAllStudents\").then((response) => {\r\n            this.Students = response.data;\r\n          });\r\n          break;\r\n        case \"prof\":\r\n          this.$store.dispatch(\"getAllProfs\").then((response) => {\r\n            this.Profs = response.data;\r\n          });\r\n          break;\r\n        case \"admin\":\r\n          this.$store.dispatch(\"getAllAdmins\").then((response) => {\r\n            this.Admins = response.data;\r\n          });\r\n          break;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":""}]}